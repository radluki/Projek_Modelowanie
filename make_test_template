all: test_template
test_template: blum_blum.o test.o Test.o
	g++ blum_blum.o test.o Test.o -o test_template
	make -f make_test_template clear
blum_blum.o: blum_generator.cpp
	g++ blum_generator.cpp -std=c++11 -c -o blum_blum.o
test.o:
	g++ test.cpp -std=c++11 -c
Test.o: Test.cpp
	g++ Test.cpp -c -std=c++11
clear:
	rm *~ *.o
